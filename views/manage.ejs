<!DOCTYPE html>
<html lang="en">
<head>
<link rel="shortcut icon" type="image/png" href="https://i.imgur.com/jyGY3Y9.png" />
    <meta charset="UTF-8">
    <title>Ticket Management - Xifty Dashboard</title>
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,500,700,900" rel="stylesheet">
    <link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.indigo-pink.min.css">
    <meta name="author" content="Daan Breur">
    <meta name="description" content="Xifty Dashboard">
    <link rel="stylesheet" href="../styles/styles.css">
</head>
<body>
<div class="mdl-layout mdl-js-layout mdl-layout--fixed-header">
    <header class="mdl-layout__header">
        <div class="mdl-layout__header-row">
            <%- include('./templates/nav'); %>
        </div>
    </header>
    <div class="mdl-layout__drawer">
        <span class="mdl-layout-title">Dashboard</span>
        <%- include('./templates/nav'); %>
    </div>
    <main class="mdl-layout__content">
        <!-- page content -->
        <div class="page-content">
            <div class="mdl-grid">
                <div class="mdl-cell mdl-cell--2-col"></div>
                <div class="mdl-cell mdl-cell--8-col">
                    <h1><b>Ticket Management</b></h1>
                    <hr>
                    <div class="manageContainer management_commandsContainer">
                        <h3><b>All Tickets: </b></h3>
                        <hr>
                        <ul class="commandList">
                          <% for (let i of Object.keys(tickets)) { %>
                            <li class="commandListItem">
                                <p class="commandName"><%= tickets[i].ticketName %></p>
                                <div class="commandFunction"></div>
                                <hr>
                                <p>
                                    <% if(tickets[i].ticketStatus === "closed"){ %>
                                          <span class="commandAvailableInMaintenance commandAvailableInMaintenanceFalse">Closed</span>
                                    <% }else if(tickets[i].ticketStatus === "opened"){ %>
                                        <span class="commandAvailableInMaintenance commandAvailableInMaintenanceTrue">Opened</span>
                                    <% } %>
                                </p>
                            </li>
                          <% } %>
                        </ul>
                    </div>
                </div>
                <div class="mdl-cell mdl-cell--2-col">
                </div>
            </div>
        </div>
    </main>
</div>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="../scripts/dashboard.js"></script>
<script defer src="https://code.getmdl.io/1.3.0/material.min.js"></script>
</body>
</html>
